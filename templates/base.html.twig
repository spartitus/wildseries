<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>
    <header>
            {% include 'navbar.html.twig' %}
        {% for label, messages in app.flashes %}
            {% for message in messages %}
                <div class="alert alert-{{ label }}" role="alert">
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}
    </header>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </body>
</html>
